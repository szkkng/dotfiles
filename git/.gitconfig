[user]
	name = szkkng
	email = hi@kengo.dev
[init]
	defaultBranch = main
[alias]
	st = status
	br = branch
	ci = commit
	sw = switch
	last = log -1
	logol = log --oneline
[pull]
	ff = only
[fetch]
	prune = true
[core]
	autocrlf = input
	ignorecase = false
  pager = delta
[interactive]
  diffFilter = delta --color-only
[include]
  path = ~/.config/delta/themes/catppuccin.gitconfig
[delta]
  features = catppuccin-mocha
  line-numbers = true

